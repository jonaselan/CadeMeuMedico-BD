@model IEnumerable<CadeMeuMedico.Models.Especialidade>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IDEspecialidade)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th></th>
    </tr>

@foreach (var i in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => i.IDEspecialidade)
        </td>
        <td>
            @Html.DisplayFor(modelItem => i.Nome)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=i.IDEspecialidade  }) |
            @Html.ActionLink("Delete", "Delete", new { id = i.IDEspecialidade })
        </td>
    </tr>
}

</table>
