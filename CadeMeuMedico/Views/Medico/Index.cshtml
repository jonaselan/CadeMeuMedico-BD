@model IEnumerable<CadeMeuMedico.Models.Medico>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Novo Médico", "Create")
</p>
<table>
